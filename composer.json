{
  "name": "lbacik/value-object-illuminate-validation",
  "description": "Validator wrapper (to be used with ValueObject) for Illuminate Validation class",
  "type": "library",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Łukasz Bacik",
      "email": "mail@luka.sh",
      "homepage": "https://github.com/lbacik/value-object-illuminate-validation"
    }
  ],
  "require": {
    "php": ">=8.0",
    "lbacik/value-object": "^0.4",
    "illuminate/validation": "^8"
  },
  "autoload": {
    "psr-4": {
      "Sushi\\": "src/Sushi/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "squizlabs/php_codesniffer": "^3.6"
  }
}
